# OpenAI 설정
# OPENAI_API_KEY는 실행 시 수동으로 입력받습니다
# OPENAI_API_KEY=your_openai_api_key_here

# Qdrant 설정
QDRANT_HOST=localhost
QDRANT_PORT=6333
COLLECTION_NAME=reactmap_nodes

# 배치 처리 설정
BATCH_SCHEDULE_TIME=02:00
BATCH_ENABLED=true

# 로깅 설정
LOG_LEVEL=INFO
LOG_FILE=roadmap_batch.log

# AI 기능 설정
ENHANCEMENT_ENABLED=true
VALIDATION_ENABLED=true
NEW_PATHS_ENABLED=true

# HTML 출력 설정
HTML_OUTPUT_DIR=.
HTML_AUTO_OPEN=true

# OpenAI 모델 설정
OPENAI_MODEL=gpt-3.5-turbo
OPENAI_MAX_TOKENS=1000
OPENAI_TEMPERATURE=0.7 